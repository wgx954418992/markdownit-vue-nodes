{
  "name": "markdownit-vue-nodes",
  "version": "1.0.0",
  "description": "Render markdown-it tokens as Vue 3 VNodes for incremental and key-stable updates.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wgx954418992/markdownit-vue-nodes"
  },
  "keywords": [
    "markdown-it",
    "vue3",
    "vnode",
    "markdown",
    "renderer",
    "utility",
    "码友邦AI"
  ],
  "author": "码友邦AI <954418992@qq@qq.com>",
  "license": "Apache-2.0",
  "homepage": "https://ai.mayoubang.cn/?_s=mkn",
  "peerDependencies": {
    "markdown-it": "^13.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "markdown-it": "^13.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --declaration --emitDeclarationOnly false && vite build --lib",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  }
}
